{ 
    "name": "GeoIP Scavenger Hunt",
    "description": "Get a taste of the interconnected web! Try to connect to servers from all around the globe!",
    "version": "1.0.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "permissions": [
        "webRequest",
        "storage",
        "geolocation",
        "<all_urls>"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "386": "./icons/icon.png",
        "128": "./icons/icon_128.png",
        "48": "./icons/icon_48.png",
        "32": "./icons/icon_32.png",
        "16": "./icons/icon_16.png"
    }
}