{
    "presets": [
        [
            "@babel/preset-env",
            {
                /*
                "targets": {
                    "esmodules": true
                }
                */
                "targets": "defaults"
            }
        ],
        "minify"
    ],
    "plugins": [
        "@babel/plugin-proposal-object-rest-spread",
        [
            "@babel/plugin-transform-runtime",
            {
                "regenerator": true,
                "corejs": 3
            }
        ]
    ]
}